  "main": "index.js",
  "scripts": {
    "clear-dist": "rm -rf dist/*",
    "clear-docs": "rm -rf docs/*",
    "compile": "npm run clear-dist && tsc",
    "compile-test": "tsc -p tsconfig.test.json",
    "gen-docs": "npm run clear-docs && typedoc --out docs --mode modules",
    "test": "npm run compile-test && mocha -r ts-node/register test/*.ts",
    "testjs": "mocha test/*/*.js",
    "deploy": "node ./node_modules/.bin/deploy-aws service --aws-config-file=.deploy-aws-cfg.json --aws-accountId=266895356213"
  },
