  "devDependencies": {
    "@skazska/deploy-aws": "0.0.3",
    "@types/aws-lambda": "^8.10.31",
    "@types/node": "^12.6.9",
    "@types/validator": "^10.11.2",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "mocha": "^5.2.0",
    "sinon": "^7.3.1",
    "sinon-chai": "^3.3.0",
    "typescript": "^3.5.3"
